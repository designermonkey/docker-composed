{
  "name": "micron/tenancy",
  "description": "Micron Research Multi Tenancy",
  "type": "micron-library",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {},
  "require": {
    "php": "^7.4|^8.0",
    "robmorgan/phinx": "^0.12.7",
    "symfony/console": "^5.3",
    "symfony/dotenv": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "MicronResearch\\Tenancy\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.8"
  },
  "autoload-dev": {
    "psr-4": {
      "MicronResearch\\Tenancy\\": "tests"
    }
  },
  "bin": [
    "tenancy"
  ]
}
